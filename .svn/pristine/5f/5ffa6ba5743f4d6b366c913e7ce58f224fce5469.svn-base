.mainmenu ul{
	margin:0;
	padding:0;
}

.mainmenu li{
	width:92.9px;
	height:50px;
	float:left;
	color:#191919;
	text-align:center;
	overflow:hidden;
}

.mainmenu a{
	color:#FFF;
	text-decoration:none;
    font-size:16px;
}

.mainmenu p{
	padding:5px 0px 10px 0px;
}

.mainmenu .subtext{
		padding-top:20px;
}

/*Menu Color Classes*/
.mainmenu .green{background:#6AA63B url('images/green-item-bg.jpg') top left no-repeat;}
.mainmenu .yellow{background:#FBC700 url('images/yellow-item-bg.jpg') top left no-repeat;}
.mainmenu .red{background:#D52100 url('images/red-item-bg.jpg') top left no-repeat;}
.mainmenu .purple{background:#5122B4 url('images/purple-item-bg.jpg') top left no-repeat;}
.mainmenu .blue{background:#0292C0 url('images/blue-item-bg.jpg') top left no-repeat;}